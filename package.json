{
  "name": "template-base-ts",
  "version": "1.0.0",
  "description": "Base project for implementation of REST API in typescript",
  "main": "dist/server.js",
  "scripts": {
    "build": "gulp build",
    "build:doc": "typedoc --options typedoc.json",
    "start": "node dist/server.js",
    "start:dev": "gulp dev",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "nyc --clean --all --reporter=text mocha --opts test/mocha.opts test/**/*.spec.ts",
    "test:integration": "mocha --opts test/mocha.opts test/integration/**/*.spec.ts",
    "test:unit": "mocha --opts test/mocha.opts test/unit/**/*.spec.ts",
    "test:cov": "nyc --clean --all --reporter=html --reporter=text mocha --opts test/mocha.opts test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "microservice",
    "nodejs",
    "typescipt",
    "express"
  ],
  "author": "NUTES/UEPB",
  "license": "MIT",
  "nyc": {
    "include": [
      "**/*.tsx",
      "**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/*",
      "config/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "amqp-client-node": "^1.0.11",
    "amqp-ts": "^1.8.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-interceptor": "^1.2.0",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "mongoose-gridfs": "^1.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "papaparse": "^5.4.1",
    "query-strings-parser": "^2.1.13",
    "reflect-metadata": "^0.2.1",
    "sinon": "^7.5.0",
    "sinon-mongoose": "^2.3.0",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.11",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^10.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/swagger-ui-express": "^4.1.6",
    "better-docs": "^2.7.3",
    "chai": "^5.0.3",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jsdoc": "^4.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  }
}
